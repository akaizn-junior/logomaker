{
  "setupFilesAfterEnv": ["./.enzyme.config.js"],
  "transform": {
    ".*\\.css$": "./jest-skipcss.js",
    ".*\\.(js|jsx)$": "babel-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "client/**/*.{js,jsx}",
    "!**/node_modules/**",
    "!client/app/icons/**"
  ],
  "coverageReporters": ["html", "lcov", "text-summary"]
}
